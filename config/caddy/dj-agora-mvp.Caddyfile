static.local.agora.gdn {
	import cors https://app.local.agora.gdn

	# Reverse proxy all requests to the static files server
	reverse_proxy host.containers.internal:5173
	import cloudflare_tls
}

app.local.agora.gdn {
	# Reverse proxy all requests to the Gunicorn socket
	# Socket is managed by systemd `gunicorn_mvp.socket`
	# reverse_proxy unix//run/gunicorn_mvp.sock

	# Temporary reverseproxy to dev server
	reverse_proxy host.containers.internal:8000
	import cloudflare_tls
}
